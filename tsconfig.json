{
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "importHelpers": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true
  }
}
